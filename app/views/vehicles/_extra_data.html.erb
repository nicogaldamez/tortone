<div class="box box-primary">

  <div class="box-header with-border">
    <h3 class="box-title">
      <span class="fa fa-file-text-o"></span>
      Otros
    </h3>
  </div>

  <div class="box-body">

    <div class="row">
      <div class="col-md-12">
        <fieldset class="form-group">
          <%= f.check_box :is_hdi %>
          <%= f.label :is_hdi %>
        </fieldset>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <fieldset class="form-group">
          <%= f.check_box :has_automatic_transmission %>
          <%= f.label :has_automatic_transmission %>
        </fieldset>
      </div>
    </div>


  </div>

</div>
